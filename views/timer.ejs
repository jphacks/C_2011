<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title><!--システム名-->aaa_OnlineTimer</title>
        <script src="/send_url.js"></script>
        <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
        <script src="javascripts/timer.js" defer></script>
    </head>

    <body>
        <header>
            <a href="/"><!--システム名-->aaa</a>
            <% console.log("システム名決まってないよーーーーーーーーー") %>
        </header>

        <div id="timer">
            <div id="timer-title">
                <div>オンラインタイマー</div>
            </div>

            <div id="timer-body">
                <span class="hours">00</span>
                <span>:</span>
                <span class="minutes">00</span>
                <span>:</span>
                <span class="seconds">00</span>
                <span>.</span>
                <span class="milliseconds">00</span>
            </div>

            <button class="timer-btn" id="timer-start">開始</button>
            <button class="timer-btn" id="timer-end">停止</button>
            <style>
                #timer-end {
                    display : none ;
                }
            </style>
    
        </div>

    </body>
</html>